using System;

public class Program
{
	public static void Main()

	{ 

		Console.WriteLine("Введить координаты вершины А : ");

		double x1 = double.Parse(Console.ReadLine());
		double y1 = double.Parse(Console.ReadLine());

		Console.WriteLine("Введить координаты вершины B : ");
		double x2 = double.Parse(Console.ReadLine());
		double y2 = double.Parse(Console.ReadLine());


		Console.WriteLine("Введить координаты вершины С : ");
		double x3 = double.Parse(Console.ReadLine());
		double y3 = double.Parse(Console.ReadLine());



		double AB = Math.Sqrt(Math.Pow(x1 - x2, 2) + Math.Pow(y1 - y2, 2));
		double BC = Math.Sqrt(Math.Pow(x2 - x3, 2) + Math.Pow(y2 - y3, 2));
		double AC = Math.Sqrt(Math.Pow(x1 - x3, 2) + Math.Pow(y1 - y3, 2));


		double result = AB + BC + AC;


		Console.WriteLine("Периметр равен = {0:f2};", result);
	}
